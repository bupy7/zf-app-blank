{
    "name": "bupy7/zf-app-blank",
    "description": "A blank application Zend Framework 3.",
    "version": "1.1.0",
    "type": "project",
    "license": "BSD 3-Clause",
    "keywoards": [
        "zend",
        "zf",
        "zf3",
        "framework",
        "skeleton",
        "blank",
        "application",
        "app"
    ],
    "authors": [
        {
            "name": "Vasilij Belosludcev",
            "email": "bupy765@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "zendframework/zend-mvc": "^3.0.1",
        "zendframework/zend-mvc-i18n": "^1.0",
        "zendframework/zend-navigation": "^2.8.1",
        "zendframework/zend-validator": "^2.8",
        "zendframework/zend-inputfilter": "^2.7.2",
        "zendframework/zend-crypt": "^3.0",
        "zendframework/zend-authentication": "^2.5.3",    
        "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
        "zendframework/zend-mvc-plugin-identity": "^1.0",
        "zendframework/zend-mvc-plugin-prg": "^1.0",
        "zendframework/zend-mvc-plugin-fileprg": "^1.0",
        "zendframework/zend-json": "^3.0",
        "widmogrod/zf2-assetic-module": "2.0.0",
        "zf-commons/zfc-rbac": "2.6.1",
        "doctrine/doctrine-orm-module": "1.1.0",
        "doctrine/migrations": "dev-master",
        "bupy7/zf-form": "1.0.0",
        "kokspflanze/zfc-twig": "1.3.1",
        "zendframework/zend-mvc-console": "1.1.11"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src",
            "ExAssetic\\": "module/ExAssetic/src",
            "ExDebugBar\\": "module/ExDebugBar/src",
            "ExDoctrine\\": "module/ExDoctrine/src",
            "ExRbac\\": "module/ExRbac/src",
            "ExTwig\\": "module/ExTwig/src",
            "ExValidate\\": "module/ExValidate/src",
            "User\\": "module/User/src"
        }
    },
    "require-dev": {
        "snapshotpl/zf-snap-php-debug-bar": "0.12.1",
        "friendsofphp/php-cs-fixer": "1.13.1"
    },
    "scripts": {
        "phpcs": "php vendor/bin/php-cs-fixer fix"
    }
}
